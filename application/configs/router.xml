<?xml version="1.0" encoding="UTF-8"?>
<configdata xmlns:zf="http://framework.zend.com/xml/zend-config-xml/1.0/"
	xmlns:xi="http://www.w3.org/2003/XInclude">
	<production>
		<router>
			<routes>
				<home>
					<route>/</route>
					<defaults>
						<module>cms</module>
						<controller>content</controller>
						<action>read</action>
						<template-name>home</template-name>
					</defaults>
				</home>
				<webdesign>
					<route>webdesign/:page</route>
					<defaults>
						<module>portfolio</module>
						<controller>content</controller>
						<action>read</action>
						<category>webdesign</category>
						<page>1</page>
					</defaults>
					<reqs>
						<page>\d+</page>
					</reqs>
				</webdesign>
				<print>
					<route>print/:page</route>
					<defaults>
						<module>portfolio</module>
						<controller>content</controller>
						<action>read</action>
						<category>print</category>
						<page>1</page>
					</defaults>
					<reqs>
						<page>\d+</page>
					</reqs>
				</print>
				<miscellaneous>
					<route>miscellaneous/:page</route>
					<defaults>
						<module>portfolio</module>
						<controller>content</controller>
						<action>read</action>
						<category>miscellaneous</category>
						<page>1</page>
					</defaults>
					<reqs>
						<page>\d+</page>
					</reqs>
				</miscellaneous>
				<vita>
					<route>vita</route>
					<defaults>
						<module>cms</module>
						<controller>content</controller>
						<action>read</action>
						<template-name>vita</template-name>
					</defaults>
				</vita>
				<contact>
					<route>contact</route>
					<defaults>
						<module>contact</module>
						<controller>contact-form</controller>
						<action>read</action>
					</defaults>
				</contact>
				<impressum>
					<route>impressum</route>
					<defaults>
						<module>cms</module>
						<controller>content</controller>
						<action>read</action>
						<template-name>impressum</template-name>
					</defaults>
				</impressum>
			</routes>
		</router>
	</production>
</configdata>